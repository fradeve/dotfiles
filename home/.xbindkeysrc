# Examples of commands:

"xbindkeys_show" 
  control+shift + q

"mpc toggle"
    m:0x40 + c:54
    Mod4 + c

"mpc next"
    m:0x40 + c:31
    Mod4 + i

"mpc prev"
    m:0x40 + c:30
    Mod4 + u

"mpc volume -2"
    m:0x40 + c:94
    Mod4 + less

"mpc volume +2"
    m:0x40 + c:52
    Mod4 + z

"konsole"
    m:0x40 + c:28
    Mod4 + t

"dolphin"
    m:0x40 + c:43
    Mod4 + h

"pamixer --toggle-mute"
    m:0x40 + c:38
    Mod4 + a

"pamixer --decrease 5"
    m:0x40 + c:39
    Mod4 + s

"pamixer --increase 5"
    m:0x40 + c:40
    Mod4 + d

"xfce4-session-logout"
 m:0x40 + c:24
    Mod4 + q

"asus-kbd-backlight down"
    m:0x0 + c:237
    XF86KbdBrightnessDown

"asus-kbd-backlight up"
    m:0x0 + c:238
    XF86KbdBrightnessUp

"hamster stop && notify-send -i hamster-time-tracker -t 3000 'Stopped Hamster activity'"
    m:0x40 + c:26
    Mod4 + e

"yakuake"
    m:0x0 + c:123
    XF86AudioRaiseVolume

"kupfer"
    m:0x40 + c:133
    Mod4 + Super_L

"kupfer"
    m:0x4 + c:94
    Control + less

"xset dpms force off"
    m:0x0 + c:76
    F10

